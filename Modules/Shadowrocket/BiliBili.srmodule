http-response ^https:\/\/(?:app\.bilibili\.com|grpc\.biliapi\.net)\/bilibili\.community\.service\.dm\.v1\.DM\/DmSegMobile script-path=https://github.com/ifflagged/nBlock/raw/main/Walalala/Scripts/bilibili/protoC.js, requires-body=true, binary-body-mode=true, tag=会员特效弹幕处理
http-response ^https:\/\/(?:app\.bilibili\.com|grpc\.biliapi\.net)\/bilibili\.community\.service\.dm\.v1\.DM\/DmView script-path=https://github.com/ifflagged/nBlock/raw/main/Walalala/Scripts/bilibili/protoC.js, requires-body=true, binary-body-mode=true, tag=移除交互式弹幕
http-response ^https:\/\/(?:app\.bilibili\.com|grpc\.biliapi\.net)\/bilibili\.main\.community\.reply\.v1\.Reply\/MainList script-path=https://github.com/ifflagged/nBlock/raw/main/Walalala/Scripts/bilibili/protoC.js, requires-body=true, binary-body-mode=true, tag=移除评论区广告
http-response ^https:\/\/(?:app\.bilibili\.com|grpc\.biliapi\.net)\/bilibili\.polymer\.app\.search\.v1\.Search\/SearchAll$ script-path=https://github.com/ifflagged/nBlock/raw/main/Walalala/Scripts/bilibili/protoC.js, requires-body=true, binary-body-mode=true, tag=移除搜索结果广告
http-response ^https:\/\/app\.bilibili\.com\/x\/v2\/splash\/(?:brand\/list|event\/list2|list|show) script-path=https://github.com/ifflagged/nBlock/raw/main/Walalala/Scripts/bilibili/json.js, requires-body=true, tag=移除开屏广告
